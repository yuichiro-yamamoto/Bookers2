<%= render 'books/list', user:@user, book:@book %>
         <div class="col-md-9">
          <h1>Books</h1>
           <table class="table table-hover table-inverse">
            <thead>
              <th>Title</th>
              <th>Opinion</th>
            </thead>
            <tbody>
              <% @user.books.each do |book| %>
                <tr>
                  <td><%= link_to book.title, book_path(book.id) %></td>
                  <td><%= link_to book.body, book_path(book.id) %></td>
                </tr>
              <% end %>
           </tbody>

          </table>
         </div>
